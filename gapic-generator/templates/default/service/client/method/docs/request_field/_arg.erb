<%- assert_locals field -%>
<%- key, value = field -%>
<%- if value.comment -%>
<%= indent value.comment, "# " %>
<%- end -%>
<%- if value.value_is_file && !value.input_parameter -%>
<%= key %> = File.read <%= value.value %>
<%- else -%>
<%= key %> = <%= value.input_parameter || value.value %>
<%- end -%>
