<%# Declare a module. %>
module <%= @util.upper_camel @namespaces.first %>
<%# Put content if there are no more modules. %>
<% if @namespaces.length <= 1 %>
<%= @content -%>
<% else %>
<%# Recursively call this template ensuring to pass @util %>
<%= @util.modules.evaluate(
  util: @util,
  content: @content,
  namespaces: @namespaces[1..-1]) -%>
<% end %>
end
