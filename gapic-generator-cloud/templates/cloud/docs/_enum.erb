<%- assert_locals enum -%>
<%- if enum.doc_description -%>
<%= indent enum.doc_description, "# " %>
<%- end -%>
module <%= enum.name %>
  <%- enum.values.each do |value| -%>

  <%- if value.doc_description -%>
  <%= indent value.doc_description, "# " %>
  <%- end -%>
  <%= value.name %> = <%= value.number %>
  <%- end -%>
end